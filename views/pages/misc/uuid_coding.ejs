<%
encodeUUID = function(id) {
	const base64id = Buffer.from((id = (id.replace(/-/g, ''))), 'hex').toString('base64url');
	return base64id;
}
%>

<%
transformDate = function(originalDate) {
	let newDate = new Date(originalDate)
	newDate = newDate.toLocaleString('en-GB');
	return newDate;
}
%>

<%
transformDatewithhours = function(originalDate) {
	let newDate = new Date(originalDate)
	return newDate;
}
%>