<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title></title>
</head>
<body>
    <div class="header">
        <%- include('../misc/header.ejs') %>
    </div>
    <div class="main_container">
        <div class="categories">
            <ul id="category_list">
                <% sections.forEach(element => { %>
                    <a href='/subsection/<%= element.id %>'><%- element.name %></a>
					<a href="/section/edit?sectionId=<%=element.id%>">Edit</a>
                <% }) %>
            
            </ul>
        </div>
			<a href="/section/create">Add section</a>
            <a href="/section">Return to sections</a>
    </div>
</body>
</html>